<% content_for :html_head do %>
<%= stylesheet_link_tag("cms/administration.css") %>
<%= stylesheet_link_tag("cms/content_library.css") %>
<% end %>
<%= render :partial => 'layouts/cms/head' %>
<body>
  <div id="wrapper">
    <%= render_cms_toolbar(:administration) %>
    <div id="main">
    <div class="top_cap_menu"></div>
      <div id="menu">
        <%= render :partial => 'cms/shared/admin_sidebar' %>                      
      </div>
      <div id="contentwrap">
	<div id="functions">
	  <h1><%= @toolbar_title %></h1>
	  <%= yield :functions %>

	</div>
	<br clear="all" />
	<div class="top_cap_content"></div>
      <div id="content">
	<div class="pad">
          <%= yield %>
	</div>
      </div>
    </div>
    <br clear="all" />
    <div class="bottom_cap_content"></div>
    <%= render :partial => 'layouts/cms/footer' %>
  </div>
</div>
</body>
</html>
